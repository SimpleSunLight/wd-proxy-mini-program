
.cart-number{
  padding-bottom: 26rpx;
  border-bottom: 2rpx solid #efefef;
}

.cart-number-title {
  /* width: 48rpx; */
  height: 34rpx;
  display: block;
  font-size: 24rpx;
  color: #979797;
  margin-top: 26rpx;
  margin-left: 42rpx;
}

.cart-number-btn {
  margin-top: 16rpx;
  margin-left: 40rpx;
  display: flex;
  flex-wrap: wrap;
}

.cart-number-btn-item {
  margin-right: 16rpx;
  text-align: center;
  height: 52rpx;
  line-height: 52rpx;
  font-size: 24rpx;
  display: block;
  color: #999;
  border-radius: 26rpx;
  width: 120rpx !important;
  padding: 0 !important;
  border: 2rpx solid #efefef;
  background: #f5f5f5;
}

.hight-color {
  color: #FD4C72;
  background: #FFD7E0;
}