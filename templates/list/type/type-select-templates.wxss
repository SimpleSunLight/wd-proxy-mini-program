.type-select-view {
  width: 50%;
  height: 60rpx;
  display: flex;
  align-items: center;
  border-radius: 10rpx;
  background-color: #fff;
  position: relative;
}

.type-select-view-icon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 8rpx;
}

.type-title {
  width: 140rpx;
  height: 40rpx;
  color: #999;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
  font-family: PingFangSC-Regular, PingFang SC;
}

.type-title-on {
  color: #666;
  width: 242rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  padding-left: 8rpx;
  padding-right: 8rpx;
  white-space: nowrap;
  overflow: hidden;
  display: inline-block;
  text-overflow: ellipsis;
  font-family: PingFangSC-Regular, PingFang SC;
}

.type-del {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  right: 20rpx;
  top: 16rpx;
}

.pa-f{
  width: 100%;
  display: flex;
  align-items: center;
}